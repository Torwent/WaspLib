type
  TWaspLib = record
    LogFile: String;
    Activity: TCountDown;

    GameSettings: record
      HighestPitch: Boolean;
      MaxBrightness: Boolean;
      RoofsHidden: Boolean;
      BankWithdrawX: Int32;
      XPBarIsSetup: Boolean;
      MouseWheel: Boolean;
      MouseWheelConfirmed: Boolean;
      MagicFiltersRemoved: Boolean;
    end;

    XP: record
      Start: Int32;
      Previous: Int32;
      IsSetup: Boolean;
    end;
  end;

const
  WL_DATAPATH = DATAPATH + 'WaspLib' + DirectorySeparator;
var
  WL: TWaspLib;

